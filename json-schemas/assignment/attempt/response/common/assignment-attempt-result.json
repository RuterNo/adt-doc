{
  "$id": "https://schemas.ruter.no/adt/ota/api/v3.0/assignment/attempt/response/common/assignment-attempt-result.json",
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "AssignmentAttemptResult",
  "type": "object",
  "additionalProperties": true,
  "properties": {
    "code": {
      "type": "string",
      "examples": [
        "OK",
        "TIMEOUT",
        "BAD_REQUEST",
        "NOT_FOUND",
        "NOT_SUPPORTED",
        "NOT_AUTHORIZED",
        "INTERNAL_SERVER_ERROR"
      ]
    },
    "reason": {
      "type": "string"
    },
    "description": {
      "type": "string"
    },
    "details": {
      "type": "array",
      "items": {
        "$ref": "assignment-attempt-result-details.json"
      }
    }
  }
}
