{
  "$id": "https://schemas.ruter.no/adt/ota/api/v3.0/assignment/common/assignment-header.json",
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "AssignmentHeader",
  "additionalProperties": true,
  "type": "object",
  "required": [
    "traceId",
    "timestamp",
    "apiVersion",
    "vehicleId"
  ],
  "properties": {
    "traceId": {
      "$id": "#/properties/traceId",
      "type": "string",
      "description": "A unique identifier - UUID"
    },
    "timestamp": {
      "$id": "#/properties/timestamp",
      "type": "string"
    },
    "apiVersion": {
      "$id": "#/properties/apiVersion",
      "type": "string"
    },
    "vehicleId": {
      "$id": "#/properties/vehicleId",
      "type": "string"
    }
  }
}
