{
  "$id": "https://schemas.ruter.no/adt/ota/api/v2.1/proprietary-extensions/dpi-acknowledge.json",
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "DPIAcknowledge",
  "type": "object",
  "required": [
    "eventTimestamp",
    "traceId",
    "type",
    "payload"
  ],
  "description": "The DPI Ack topic.",
  "additionalProperties": true,
  "properties": {
    "eventTimestamp": {
      "$id": "#/properties/eventTimestamp",
      "type": "string",
      "description": "ISO 8601",
      "format": "date-time"
    },
    "traceId": {
      "$id": "#/properties/traceId",
      "$comment": "Since 3.0",
      "type": "string",
      "description": "A unique identifier - UUID"
    },
    "type": {
      "$id": "#/properties/type",
      "type": "string",
      "examples": [
        "AUDIO",
        "DESTINATION"
      ]
    },
    "payload": {
      "$id": "#/properties/payload",
      "type": "object",
      "description": "Destination text strings."
    }
  }
}
